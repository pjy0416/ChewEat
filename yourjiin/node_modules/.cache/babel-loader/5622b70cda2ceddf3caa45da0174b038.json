ent {\r\n        margin-bottom : 15px;\r\n        font-weight: bold;\r\n    }\r\n    .left-main-allerginic {\r\n        font-weight: bold;\r\n    }\r\n    .item-text-main-right {\r\n        width : 50%;\r\n        vertical-align: top;\r\n    }\r\n    .information-like , .information-review{ \r\n        font-weight: bold;\r\n    }\r\n    \r\n    .information-item {\r\n        font-weight: 400;\r\n    }\r\n    .right-main-comment {\r\n        font-weight: bold;\r\n    }\r\n    .information-p {\r\n        margin-top : 10px; \r\n        font-weight : 300;\r\n        padding : 0 0 0 32px;\r\n    }\r\n    .information-p:nth-last-child(1){\r\n        margin-left : 5px;\r\n    }\r\n/* /////////////////////////////////////// */\r\n    .item-chart {\r\n        width : 60%;\r\n        display : block;\r\n        margin : 50px auto 70px auto;\r\n    }\r\n    .chart-header {\r\n        text-align: center;\r\n        font-size: 1.8rem;\r\n        font-weight: 900;\r\n        margin-bottom : 30px;\r\n    }\r\n\r\n/* //////////////////////////////////////// */\r\n    @media(max-width : 1000px) {\r\n        .item-chart {\r\n            width : 80%;\r\n        }\r\n    }\r\n    @media (max-width : 700px) {\r\n        .item-image-section , .item-text-section {\r\n            display : block;\r\n        }\r\n        .item-image-section {\r\n            width : 40%;\r\n        }\r\n        .item-text-section {\r\n            width : 100%;\r\n        }\r\n        .item-text-header {\r\n            width : 60%;\r\n            font-size : 1.3rem;\r\n        }\r\n        .item-chart {\r\n            width : 100%;\r\n        }\r\n        .chart-main {\r\n            width : 100%;\r\n        }\r\n    }\r\n    @media (max-width : 520px) {\r\n        .item-image-section {\r\n            width : 100%;\r\n        }\r\n        .item-img {\r\n            width : 100%;\r\n        }\r\n        .item-text-main-left , .item-text-main-right {\r\n            font-size : 0.9rem;\r\n        }\r\n        .i